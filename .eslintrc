{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-case-declarations": 0,
    "no-confusing-arrow": 0,
    "global-require": 0,
    "comma-dangle": 0,
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "consistent-return": 0,
    "react/jsx-boolean-value": 0,
    "react/jsx-no-bind": 0,
    "react/forbid-prop-types": 0,
    "class-methods-use-this": 0,
    "react/prefer-stateless-function": 0,
    "react/require-extension": "off"
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "arrowFunctions": true,
    "experimentalObjectRestSpread": true,
    "jsx": true
  }
}
